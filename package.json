{
  "name": "buildtool",
  "version": "2.5.0",
  "description": "Buildtowol",
  "main": "dummy.js",
  "scripts": {
    "build": "onelua . --no-minify --prepend-meta",
    "build:watch": "onchange \"src/**/*.lua\" -i -- onelua . --no-minify --prepend-meta",
    "minify": "onelua . -o buildtool-minified.lua",
    "poeditor:build": "ts-node tools/build-translations.ts",
    "poeditor:fetch": "ts-node -r dotenv/config tools/poeditor-fetch.ts"
  },
  "onelua": {
    "main": "src/main.lua",
    "output": "buildtool.lua"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mousetool/cookie-ui": "link:./src/util/staging/cookie-ui",
    "@mousetool/linkedlist": "link:src/util/staging/linkedlist",
    "@mousetool/map": "link:src/util/staging/map",
    "@mousetool/mousebase": "link:src/util/staging/mousebase",
    "@mousetool/mousexml": "^1.0.3",
    "@mousetool/reactive": "link:src/util/staging/reactive",
    "@mousetool/event-emitter": "link:src/util/staging/event-emitter"
  },
  "devDependencies": {
    "@mousetool/onelua": "^1.2.0",
    "@mousetool/tfm-vscode": "^2.0.4",
    "@types/node": "^17.0.23",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "onchange": "^7.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
